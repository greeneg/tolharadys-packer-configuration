  <scripts>
    <init-scripts config:type="list">
      <script>
        <notification>Configuring Software Repositories. Please be patient...</notification>
        <interpreter>shell</interpreter>
        <source><![CDATA[
#!/bin/bash
{
  /usr/bin/zypper rr 1
  /usr/bin/zypper ar https://download.opensuse.org/distribution/leap/42.3/repo/oss oss
  /usr/bin/zypper ar https://download.opensuse.org/update/leap/42.3/oss oss-updates
} > /root/post-install-output.log 2>/root/post-install-error.log
]]>
        </source>
      </script>
    </init-scripts>
